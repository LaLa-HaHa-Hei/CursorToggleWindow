<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <ApplicationIcon>icon\64.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\32.ico" />
    <None Remove="Assets\QQGroup.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="icon\64.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\32.ico" />
    <Resource Include="Assets\QQGroup.jpg" />
  </ItemGroup>

</Project>
